-- Node with array slices 
node x75 (a: int^5) returns (d1: int; a23: int^2);
const A = [1, 2, 3, 4, 5, 6];
var
  b1 : int;
  b2, b3: int^3;
  b4 : int^1;
  b5: int; 
let
  d1 = a[0];       -- type int
  a23 = a[2..3];   -- type int^1
  b1 = A[1];       -- type int
  b2 = A[2..4];    -- type int^2
  b3 = A[4..2];    -- type int^2
  b4 = A[2..2];    -- type int^1 
  b5 = A[2..4][0]; -- type int
tel


node abs (a: int) returns (b: int);
let
  b = if a > 0 then a else -a;
tel

node X (a: int) returns (b: int);
var OK: bool;
let
  b = 2 * a;
  OK = b >= 0;
  --%PROPERTY OK;
tel

node Y(a: int; clk: bool) returns (b: int);
var OK: bool;
let
  b = condact(clk, X(abs(a)), 1);
  OK = b > 1;
  --%PROPERTY OK;
tel